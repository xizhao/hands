{
  "name": "@hands/starter",
  "version": "1.0.0",
  "description": "A bare-bones hands starter app",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "team@hands.app",
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "npm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && npm run clean && npm run build && wrangler deploy",
    "generate": "rw-scripts ensure-env && wrangler types --include-runtime false",
    "check": "npm run generate && npm run types",
    "types": "tsc"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.561.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-server-dom-webpack": "19.2.3",
    "rwsdk": "1.0.0-beta.41",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.16.1",
    "@cloudflare/workers-types": "4.20251202.0",
    "@types/node": "~24.5.2",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.8.3",
    "vite": "7.2.6",
    "wrangler": "4.52.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
