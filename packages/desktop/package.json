{
  "name": "@hands/desktop",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "_comment:dev": "NOTE: Editor is NOT built here - runtime spawns editor dev server. Both must run in dev mode for RSC protocol compatibility.",
    "dev:server": "bun run ../agent/src/index.ts",
    "dev:all": "concurrently -n agent,vite -c blue,green \"bun run dev:server\" \"bun run dev\"",
    "build": "tsc -b && vite build && bun run build:editor",
    "build:editor": "cd ../editor && bun run build:sandbox",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:docs": "cd ../../docs && bun run build",
    "cleanup": "lsof -ti:1420,55300 | xargs kill -9 2>/dev/null || true",
    "tauri:dev": "bun run cleanup && tauri dev",
    "tauri:build": "bun run build:docs && tauri build"
  },
